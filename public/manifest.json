{
  "manifest_version": 3,
  "name": "specsearch",
  "description": "specsearch",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["http://*.google.com/*", "https://*.google.com/*"],
      "js": ["/assets/js/initializeUI.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["/react/index.js", "/react/vendor.js", "/react/index.css"],
      "matches": ["http://*.google.com/*", "https://*.google.com/*"]
    }
  ]
}
